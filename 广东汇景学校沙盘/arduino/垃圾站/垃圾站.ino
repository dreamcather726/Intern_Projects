void loop(){
  if (mySerial1.available() && 等待回答) {
    delay(20);
    for (int i = 0; i <= 5; i = i + (1)) {
      rec[i] = mySerial1.read();
      Serial.write(rec[i]);
    }
    回答 = 0;
    if (rec[0] == 0x55) {
      回答 = rec[3];

    }
    正确答案 = 0;
